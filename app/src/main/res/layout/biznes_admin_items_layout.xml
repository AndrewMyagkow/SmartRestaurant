<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="346dp"
        android:layout_height="70dp"
        android:background="@drawable/backgrount">
    <RelativeLayout
        android:layout_width="346dp"
        android:layout_height="70dp"
        android:layout_margin="7dp"
        android:background="@drawable/inputdate">
        <TextView
            android:text="Дата"
            android:layout_width="60dp"
            android:layout_height="25dp"
            android:textColor="#B1B2B3"
            android:textSize="15dp"
            android:layout_marginTop="25dp"
            android:gravity="center"
            android:layout_marginStart="10dp"
            />
        <TextView
            android:text="   ID:   "
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginStart="10dp"
            android:textColor="#B1B2B3"
            android:gravity="center"
            android:textSize="15dp"
            />
        <TextView
            android:id="@+id/id_zakaz_biznes"
            android:layout_width="190dp"
            android:layout_height="25dp"
            android:layout_marginStart="53dp"
            android:textColor="#B1B2B3"
            android:gravity="center"

            android:textSize="15dp"
            />

        <TextView
            android:id="@+id/date_zakaz_biznes"
            android:layout_width="160dp"
            android:layout_height="25dp"
            android:textAlignment="center"
            android:layout_marginStart="70dp"
            android:layout_marginTop="25dp"
            android:textColor="#B1B2B3"
            android:textSize="15dp"
            />
        <TextView
            android:id="@+id/bar_zakaz_biznes"
            android:layout_width="0dp"
            android:layout_height="0dp"
            />
        <TextView
            android:id="@+id/kithen_zakaz_biznes"
            android:layout_width="0dp"
            android:layout_height="0dp"
            />
        <TextView
            android:id="@+id/symma_zakaz_biznes"
            android:layout_width="0dp"
            android:layout_height="0dp"
            />
        <TextView
            android:id="@+id/table_zakaz_biznes"
            android:layout_width="0dp"
            android:layout_height="0dp"
            />



    </RelativeLayout>
    </RelativeLayout>

</com.google.android.material.card.MaterialCardView>