<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.ZalobiBook.ZalobiBookActivity"
    android:background="#52535A">


    <include layout="@layout/content_main_zalobibook"
        />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:background="#38393C"
        android:gravity="center"

        >

        <ImageView
            android:id="@+id/back_zalobibook"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="45dp"
            android:src="@drawable/back"
            tools:ignore="MissingConstraints" />
        <TextView
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="Книга жалоб и предложений"
            android:textColor="#B1B2B3" />

        <ImageView
            android:id="@+id/add_zalobibook"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_margin="45dp"
            android:src="@drawable/add"
            tools:ignore="MissingConstraints" />

    </LinearLayout>



</androidx.coordinatorlayout.widget.CoordinatorLayout>