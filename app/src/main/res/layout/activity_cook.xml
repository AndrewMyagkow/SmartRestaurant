<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Cook.CookActivity"
    android:background="#52535A">

    <include layout="@layout/content_main_cook"
        />

    <RelativeLayout
        android:layout_width="360dp"
        android:layout_height="45dp"

        android:background="#38393C"
        android:gravity="right">

        <ImageView
            android:id="@+id/setingscook"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:src="@drawable/setings"
            tools:ignore="MissingConstraints" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="360dp"
        android:layout_height="51dp"
        android:background="#38393C"
        android:layout_marginTop="523dp"
        >
        <ImageView
            android:id="@+id/writebookcook"
            android:layout_width="40dp"
            android:layout_height="40dp"
           android:layout_marginStart="140dp"
            android:layout_marginTop="7dp"
            android:src="@drawable/writebookdob"
            />
        <ImageView
            android:id="@+id/chatcook"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginTop="7dp"
            android:src="@drawable/chat"
           android:layout_marginStart="200dp"/>


    </RelativeLayout>



</androidx.coordinatorlayout.widget.CoordinatorLayout>